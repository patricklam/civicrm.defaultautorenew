<?xml version="1.0"?>
<extension key="defaultautorenew" type="module">
  <file>defaultautorenew</file>
  <name>defaultautorenew</name>
  <description>Makes the auto-renew box on the membership page default to clicked.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Lam</author>
    <email>patrick.lam@ontarioallianceofclimbers.ca</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-01-15</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments>In Progress.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Defaultautorenew</namespace>
    <format>23.02.1</format>
    <angularModule>crmDefaultautorenew</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
